{
    "name": "linktracker/module-tracking",
    "description": "Linktracker customer referral tracking",
    "type": "magento2-module",
    "license": "MIT",
    "require": {
        "php": "^7.1|^7.2|^7.3",
        "magento/framework": "~102.0.1",
        "magento/module-sales": "~102.0.1"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Linktracker\\Tracking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linktracker\\Tracking\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
